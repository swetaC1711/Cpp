#include <iostream>
using namespace std;
template <typename T>
void swapValues(T &a,T &b){
    T temp = a;
    a = b;
    b = temp;
}

int main() {
    int x = 5;
    int y = 10;
    
    cout << "Before swapping: x = " << x << ", y = " << y << endl;
    
    swapValues(x, y);
    
    cout << "After swapping: x = " << x << ", y = " << y << endl;
    
    return 0;
}
